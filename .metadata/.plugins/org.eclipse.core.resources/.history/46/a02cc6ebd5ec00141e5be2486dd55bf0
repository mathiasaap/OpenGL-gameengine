package mesh;

import rendering.LoadMesh;

public static class Rekt {

	private static final Mesh mesh;
	private static final float[] rektPos={
			-1,1,
			-1,-1,
			1,1,
			1,-1	
	};
	
	public static Rekt(LoadMesh loader)
	{
		mesh = loader.loadNewMesh(rektPos);
		
	}
	
	public static Mesh getMesh()
	{
		return mesh;
	}
	
}
